        <%- require('../../_components/mForm.ejs')({
          name: 'Bank',
          type: 'select',
          options:[{
            name: '请选择',
            value: ''
          }, {
            name: '银行1',
            value: '1'
          }],
          validate : `{'req': '请选择银行'}`
        }) %>
